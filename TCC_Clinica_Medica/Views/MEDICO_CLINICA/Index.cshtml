@model IEnumerable<TCC_Clinica_Medica.MEDICO_CLINICA>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CLINICAS.NOME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MEDICOS.CRM)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CLINICAS.NOME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MEDICOS.CRM)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
