@model List<CONSULTAS>
<div class="panel panel-primary">
    <table class="table  table-bordered">
        <tr style="background-color:#d1d1d1;font-weight: bold;">
            <td>PACIENTE</td>
            <td>DATA - HORA INICIO/FIM</td>
        </tr>
       @if (Model.Count > 0)
       {
           foreach (var item in Model)
           {
            <tr>
                <td>
                    @item.PACIENTES.USUARIOS.NOME
                </td>
                <td>
                    <span class="label label-success">@item.DATA_INICIO.Date.ToShortDateString()</span><span class="label label-danger">@item.DATA_INICIO.ToString("HH:mm") / @item.DATA_FIM.ToString("HH:mm")</span> 
                </td>
            </tr>
           }
       }
       else
       {
        <tr>
            <td colspan="5"> 
                <div class="alert alert-success" role="alert"><span class="glyphicon glyphicon-info-sign"></span>&nbsp;&nbsp;NÃO EXISTEM CONSULTAS MARCADAS PARA ESSE MEDICO</div>
            </td>
        </tr>
       }
      
    </table>
</div>
